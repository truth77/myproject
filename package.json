{
  "name": "blackford",
  "version": "1.0.0",
  "description": "",
  "main": "./server/init.js",
  "scripts": {
    "webpack-watch": "webpack serve --config webpack/webpack.dev.js --progress --color",
    "docker-dev": "concurrently \"cross-env NODE_ENV=development npm run server\" \"cross-env NODE_ENV=development npm run webpack-watch\"",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run client\" \"npm run server\"",
    "client": "cross-env NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js --progress --color",
    "server": "cross-env NODE_ENV=development nodemon ./server/init.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js --progress --profile --color",
    "start": "cross-env NODE_ENV=production node server/init.js",
    "prod": "concurrently \"npm run build\" \"npm run migrate\" \"npm run start\"",
    "migrate": "knex migrate:latest",
    "lint": "eslint client/ --ext .js --ext .jsx && eslint server/ --ext .js",
    "test": "(dropdb test_blackford --if-exists && createdb test_blackford) && NODE_ENV=test jest --forceExit",
    "watch": "(dropdb test_blackford --if-exists && createdb test_blackford) && NODE_ENV=test jest --watch *.js",
    "cover": "istanbul cover npm run test --no-default-excludes",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [],
  "author": "Rauno Freiberg",
  "license": "MIT",
